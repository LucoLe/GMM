<div class="col-md-6 col-md-offset-3 sign-in">
  <div class="panel-group" id="accordion1">
    <div class="panel panel-default">
      <a class="panel-default" data-toggle="collapse" data-parent="#accordion1" href="#collapseOne">
      <div class="panel-heading">
        <h4 class="panel-title">
          Member
        </h4>
      </div>
      </a>
      <div id="collapseOne" class="panel-collapse collapse in">
        <div class="panel-body">
          <%= link_to "Sign in", new_member_session_path, class: 'btn btn-primary' %> or
          <%= link_to "Sign up", new_member_registration_path %>
        </div>
      </div>
    </div>

    <div class="panel panel-default">
      <a class="panel-default" data-toggle="collapse" data-parent="#accordion1" href="#collapseTwo">
      <div class="panel-heading">
        <h4 class="panel-title">
          Club Owner
        </h4>
      </div>
      </a>
      <div id="collapseTwo" class="panel-collapse collapse">
        <div class="panel-body">
          <%= link_to "Sign in", new_club_owner_session_path, class: 'btn btn-primary' %> or <%= link_to "Sign up", new_club_owner_registration_path %>
        </div>
      </div>
    </div>
  </div>
</div>